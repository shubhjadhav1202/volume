#include<iostream>
using namespace std;
class vol{
    public:
    void volume(int l){
        cout << "Volume of Cube: "<< l*l*l << endl;
    }
    void volume(double l,double b,double h){
        cout << "Volume of Cuboid: " << l*b*h << endl;
    }
};

int main(){
    vol hack;
    
    hack.volume(6);
    hack.volume(2.3,4.3,1);
    
    return 0;
}
